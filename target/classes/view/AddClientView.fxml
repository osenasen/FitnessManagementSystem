<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fms.controller.AddClientController">
    <children>
        <Label layoutX="116.0" layoutY="14.0" text="First Name" />
        <TextField fx:id="firstNameField" layoutX="63.0" layoutY="34.0" prefHeight="25.0" prefWidth="173.0" />
        <Label layoutX="116.0" layoutY="69.0" text="Last Name" />
        <TextField fx:id="lastNameField" layoutX="63.0" layoutY="89.0" prefHeight="25.0" prefWidth="173.0" />
        <Button layoutX="63.0" layoutY="125.0" onAction="#handleAddClient" text="Add Client" />
        <Button layoutX="156.0" layoutY="125.0" onAction="#handleRemoveClient" text="Remove Client" />
        <TableView fx:id="clientTableView" layoutX="14.0" layoutY="160.0" prefHeight="200.0" prefWidth="270.0">
            <columns>
                <TableColumn fx:id="firstNameColumn" text="First Name" />
                <TableColumn fx:id="lastNameColumn" text="Last Name" />
            </columns>
        </TableView>
    </children>
</AnchorPane>